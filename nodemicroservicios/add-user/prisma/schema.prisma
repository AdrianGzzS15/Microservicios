generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model users {
  id              Int      @id @default(autoincrement())
  email           String?  @unique(map: "sqlite_autoindex_users_1")
  hashed_password String?
  is_active       Boolean?
}
